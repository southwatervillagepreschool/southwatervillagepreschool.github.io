{
  "name": "southwater-village-preschool-website-2020",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy-website": "gatsby clean && gatsby build && gh-pages -d public -b main -o website",
    "deploy-dev": "gatsby clean && gatsby build --prefix-paths && gh-pages -d public -o origin"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.67",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.37",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-remark-images": "^3.3.34",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-remark": "^2.8.39",
    "gatsby-transformer-sharp": "^2.5.17",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.0",
    "typeface-hind": "^1.1.13",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://southwatervillagepreschool.github.io/svhps2020web/"
  },
  "bugs": {
    "url": "https://southwatervillagepreschool.github.io/svhps2020web/issues"
  }
}
